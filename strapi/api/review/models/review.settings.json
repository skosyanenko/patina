{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "name": "Review"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "viewType": {
      "type": "integer"
    },
    "book": {
      "model": "books"
    },
    "user": {
      "via": "reviews",
      "plugin": "users-permissions",
      "model": "user"
    },
    "comments": {
      "via": "review",
      "collection": "comment"
    },
    "likes": {
      "via": "review",
      "collection": "like"
    }
  }
}
